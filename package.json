{
  "name": "next-template",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "typecheck": "tsc --noEmit",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "api:generated": "npx nad http://localhost:8080 -o api/api.ts"
  },
  "bin": "./output/server.js",
  "pkg": {
    "assets": "['./output/.next/static/**/*']",
    "scripts": "['./output/*/**.js']",
    "targets": [
      "node18-linux-arm64"
    ],
    "outputPath": "dist"
  },
  "dependencies": {
