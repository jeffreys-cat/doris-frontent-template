    "dayjs": "^1.11.9",
    "i18next": "^23.1.0",
    "i18next-browser-languagedetector": "^7.0.2",
    "lucide-react": "^0.263.1",
    "next": "^13.4.12",
    "next-qrcode": "^2.5.1",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.44.3",
    "react-i18next": "^13.0.0",
    "react-syntax-highlighter": "^15.5.0",
